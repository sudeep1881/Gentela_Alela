    <link href="~/gentela-pages/Page1/vendors/starrr/dist/starrr.css" rel="stylesheet">
    <!-- bootstrap-daterangepicker -->
    <link href="~/gentela-pages/Page1/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <link href="~/gentela-pages/page1/vendors/google-code-prettify/bin/prettify.min.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/gentela-pages/page1/build/css/custom.min.css" rel="stylesheet">

